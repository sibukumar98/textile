<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Main2Activity">

    <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="400dp"
            android:layout_height="180dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:gravity="bottom"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.454"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
                android:id="@+id/linearLayout11"
                android:layout_width="180dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_margin="5pt"
                android:gravity="center">
            <TextView
                    android:id="@+id/textView1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Color : Red"
                    android:textSize="20sp"
                    android:textColor="@color/red"
                    android:gravity="center"/>

            <TextView
                    android:id="@+id/textView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Slope : "
                    android:textSize="20sp"
                    android:textColor="@color/red"
                    android:gravity="center"/>

            <TextView
                    android:id="@+id/textView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Intercept : "
                    android:textSize="20sp"
                    android:textColor="@color/red"
                    android:gravity="center"/>

            <TextView
                    android:id="@+id/textView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Accuracy : "
                    android:textColor="@color/red"
                    android:textSize="20sp"
                    android:gravity="center"/>

        </LinearLayout>

        <LinearLayout
                android:id="@+id/linearLayout12"
                android:layout_width="180dp"
                android:layout_height="match_parent"
                android:layout_margin="5pt"

                android:gravity="bottom"
                android:orientation="vertical">

            <EditText
                    android:id="@+id/editText1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number"
                    android:hint="Enter X : "
                    android:textColorHint="@color/red"
                    android:textColor="@color/red"
                    android:textSize="20sp" />

            <EditText
                    android:id="@+id/editText2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number"
                    android:hint="Enter Y : "
                    android:textColorHint="@color/red"
                    android:textColor="@color/red"
                    android:textSize="20sp" />

            <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <Button
                        android:id="@+id/buttonXR"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="findx"
                        android:textColor="@color/red"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                <Button
                        android:id="@+id/buttonYR"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="findy"
                        android:textColor="@color/red"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" />
            </android.support.constraint.ConstraintLayout>

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="400dp"
            android:layout_height="180dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:gravity="bottom"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout1">
        <LinearLayout
                android:id="@+id/linearLayout21"
                android:layout_width="180dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_margin="5pt"
                android:gravity="center">
            <TextView
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Color : Green"
                    android:textSize="20sp"
                    android:textColor="@color/Green"
                    android:gravity="center"/>

            <TextView
                    android:id="@+id/textView5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Slope : "
                    android:textSize="20sp"
                    android:textColor="@color/Green"
                    android:gravity="center"/>

            <TextView
                    android:id="@+id/textView6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Intercept : "
                    android:textSize="20sp"
                    android:textColor="@color/Green"
                    android:gravity="center"/>

            <TextView
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Accuracy : "
                    android:textSize="20sp"
                    android:textColor="@color/Green"
                    android:gravity="center"/>

        </LinearLayout>

        <LinearLayout
                android:id="@+id/linearLayout22"
                android:layout_width="180dp"
                android:layout_height="match_parent"
                android:layout_margin="5pt"

                android:gravity="bottom"
                android:orientation="vertical">

            <EditText
                    android:id="@+id/editText3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number"
                    android:hint="Enter X : "
                    android:textColorHint="@color/Green"
                    android:textColor="@color/Green"
                    android:textSize="20sp" />

            <EditText
                    android:id="@+id/editText4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number"
                    android:hint="Enter Y : "
                    android:textColorHint="@color/Green"
                    android:textColor="@color/Green"
                    android:textSize="20sp" />

            <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <Button
                        android:id="@+id/buttonXG"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="findx"
                        android:textColor="@color/Green"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                <Button
                        android:id="@+id/buttonYG"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="findy"
                        android:textColor="@color/Green"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" />
            </android.support.constraint.ConstraintLayout>

        </LinearLayout>
    </LinearLayout>



    <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="400dp"
            android:layout_height="180dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:gravity="bottom"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2">
        <LinearLayout
                android:id="@+id/linearLayout31"
                android:layout_width="180dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_margin="5pt"
                android:gravity="center">
            <TextView
                    android:id="@+id/textView7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Color : Blue"
                    android:textSize="20sp"
                    android:textColor="@color/blue"
                    android:gravity="center"/>

            <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Slope : "
                    android:textSize="20sp"
                    android:textColor="@color/blue"
                    android:gravity="center"/>

            <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Intercept : "
                    android:textSize="20sp"
                    android:textColor="@color/blue"
                    android:gravity="center"/>

            <TextView
                    android:id="@+id/textView10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Accuracy : "
                    android:textColor="@color/blue"
                    android:textSize="20sp"
                    android:gravity="center"/>

        </LinearLayout>

        <LinearLayout
                android:id="@+id/linearLayout32"
                android:layout_width="180dp"
                android:layout_height="match_parent"
                android:layout_margin="5pt"
                android:gravity="bottom"
                android:orientation="vertical">

            <EditText
                    android:id="@+id/editText5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number"
                    android:hint="Enter X : "
                    android:textColorHint="@color/blue"
                    android:textSize="20sp"
                    android:textColor="@color/blue"/>

            <EditText
                    android:id="@+id/editText6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number"
                    android:hint="Enter Y : "
                    android:textColorHint="@color/blue"
                    android:textSize="20sp"
                    android:textColor="@color/blue"/>

            <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <Button
                        android:id="@+id/buttonXB"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="findX"
                        android:textColor="@color/blue"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                <Button
                        android:id="@+id/buttonYB"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="5dp"
                        android:text="FindY"
                        android:textColor="@color/blue"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" />
            </android.support.constraint.ConstraintLayout>

        </LinearLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>